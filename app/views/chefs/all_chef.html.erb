<div class="row">
  <div class="col-lg-12">
    <h2 class="page-header">Chef</h2>
  </div>
</div>
<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <thead>
        <tr>
          <td>Email</td>
        </tr>
      </thead>
      <tbody>
        <% @chef.each do |c| %>
        <tr>
          <td><%= c.email %></td>
          <td><%= link_to 'Edit', edit_chef_path(c), class: 'btn btn-primary' %></td>
          <td><%= link_to 'Delete', chef_path(c), method: :delete, data:{confirm: 'Are you sure?'}, class: 'btn btn-warning' %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
    <%= link_to 'ADD', new_chef_path, class:'btn btn-primary' %>
  </div>
</div>