<div class="row">
  <div class="col-lg-12">
  <% if @order_unpaid_without_table.blank? %>
    <h4 class="text-center">No Orders Unpaid Without Table </h4>
  <% else %>
    <% @order_unpaid_without_table.each do |o| %>
        <div class="col-md-3">
          <%= link_to your_order_path(o), "data-no-turbolink" => true do %>
            <div class="table-all btn btn-info">
              <h5><b>Order Id : <%= o.id %> </b></h5>
            </div>
          <% end %>
        </div>
      <% end %>
    <% end %>
  </div>
</div>
<script type="text/javascript">
  function reload(){
    window.location.reload();
  };
  setInterval('reload()', 300000);
</script>